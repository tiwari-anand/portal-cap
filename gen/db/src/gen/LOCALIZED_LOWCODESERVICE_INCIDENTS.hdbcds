using LOCALIZED_SAP_CAPIRE_DEV_INCIDENTS as LOCALIZED_SAP_CAPIRE_DEV_INCIDENTS;
using LOCALIZED_LOWCODESERVICE_STATUS as LOCALIZED_LOWCODESERVICE_STATUS;
using LOCALIZED_LOWCODESERVICE_CATEGORY as LOCALIZED_LOWCODESERVICE_CATEGORY;
using LOCALIZED_LOWCODESERVICE_PRIORITY as LOCALIZED_LOWCODESERVICE_PRIORITY;
using LOCALIZED_LOWCODESERVICE_INDIVIDUAL as LOCALIZED_LOWCODESERVICE_INDIVIDUAL;
view LOCALIZED_LOWCODESERVICE_INCIDENTS as select from LOCALIZED_SAP_CAPIRE_DEV_INCIDENTS as INCIDENTS_0 mixin {
  ___STATUS : association to LOCALIZED_LOWCODESERVICE_STATUS on ___STATUS.CODE = $projection.STATUS_CODE;
  ___CATEGORY : association to LOCALIZED_LOWCODESERVICE_CATEGORY on ___CATEGORY.CODE = $projection.CATEGORY_CODE;
  ___PRIORITY : association to LOCALIZED_LOWCODESERVICE_PRIORITY on ___PRIORITY.CODE = $projection.PRIORITY_CODE;
  ___ASSIGNEDINDIVIDUAL : association[1] to LOCALIZED_LOWCODESERVICE_INDIVIDUAL on ___ASSIGNEDINDIVIDUAL.CODE = $projection.ASSIGNEDINDIVIDUAL_CODE;
} into {
  key INCIDENTS_0.ID as ID,
  INCIDENTS_0.TITLE,
  INCIDENTS_0.DESCRIPTION,
  INCIDENTS_0.REPORTERMAIL,
  INCIDENTS_0.LTI,
  ___STATUS as STATUS,
  INCIDENTS_0.STATUS_CODE,
  INCIDENTS_0.LOCATION,
  ___CATEGORY as CATEGORY,
  INCIDENTS_0.CATEGORY_CODE,
  ___PRIORITY as PRIORITY,
  INCIDENTS_0.PRIORITY_CODE,
  INCIDENTS_0.CLOSED_ON,
  INCIDENTS_0.OPEN_ON,
  ___ASSIGNEDINDIVIDUAL as ASSIGNEDINDIVIDUAL,
  INCIDENTS_0.ASSIGNEDINDIVIDUAL_CODE
};
